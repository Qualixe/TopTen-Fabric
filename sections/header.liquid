        <ul class="nav-links">
          <li class="nav-link">
            <a class="nav-link-text" href="{{routes.root_url }}">home</a>
          </li>
          {% for link in section.settings.menu.links %}
            {% assign title = link.title | handleize %}
            {% assign has_mega_menu = false %}

            {% comment %} Check if this link has a mega menu {% endcomment %}
            {% for block in section.blocks %}
              {% assign block_title = block.settings.link | handleize %}
              {% if title == block_title and block.type == 'mega_menu' %}
                {% assign has_mega_menu = true %}
                {% break %}
              {% endif %}
