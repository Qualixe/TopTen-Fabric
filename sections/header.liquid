                    </svg>
                  </span>
                </p>

                {% for block in section.blocks %}
                  {% assign block_title = block.settings.link | handleize %}
                  {% if title == block_title and block.type == 'mega_menu' %}
                    {% assign b = block.settings %}
                    {% assign menu = b.menu %}

                    <div class="megamenu-wrapper">
                      <div class="container">
                        <div class="megamenu-top">
                          <button class="sidebar-close-btn">
                            <span class="icon">
                              <svg
                                aria-hidden="true"
                                focusable="false"
                                data-prefix="fal"
                                data-icon="long-arrow-left"
                                role="img"
                                xmlns="http://www.w3.org/2000/svg"
                                viewBox="0 0 448 512"
                                class="icon-arrow-nav"
                              >
                                <path fill="#000000"
                                	d="M136.97 380.485l7.071-7.07c4.686-4.686 4.686-12.284 0-16.971L60.113 273H436c6.627 0 12-5.373 12-12v-10c0-6.627-5.373-12-12-12H60.113l83.928-83.444c4.686-4.686 4.686-12.284 0-16.971l-7.071-7.07c-4.686-4.686-12.284-4.686-16.97 0l-116.485 116c-4.686 4.686-4.686 12.284 0 16.971l116.485 116c4.686 4.686 12.284 4.686 16.97-.001z"
                                	class=""></path>
                              </svg>
                            </span>
                            {{ link.title | downcase }}
                          </button>
                          <a href="{{ link.url }}" class="megamenu-top-btn">view all</a>
                        </div>
                        <div class="megamenu-items">
                          <div class="megamenu-item">
                            <div class="megamenu-link-wrapper">
                              {% for category_link in menu.links limit: 6 %}
                                <div class="megamenu-links">
                                  <h6 class="megamenu-links-title">{{ category_link.title }}</h6>
                                  <ul>
                                    {% if category_link.links.size > 0 %}
                                      {% for child_link in category_link.links limit: 5 %}
                                        <li>
                                          <a href="{{ child_link.url }}" class="nav-link-text">
                                            {{- child_link.title -}}
                                          </a>
                                        </li>
                                      {% endfor %}
                                    {% endif %}
                                    <li>
                                      <a href="{{ category_link.url }}" class="nav-link-text">
                                        Explore All
                                        <span class="icon">
                                          <svg
                                            width="8"
                                            height="14"
                                            viewBox="0 0 8 14"
                                            fill="none"
                                            xmlns="http://www.w3.org/2000/svg"
                                          >
                                            <path d="M7.75 7L1.5 0.75L0.624999 1.625L6 7L0.625 12.375L1.5 13.25L7.75 7Z"
                                            	fill="black" />
                                          </svg>
                                        </span>
                                      </a>
                                    </li>
                                  </ul>
                                </div>
                              {% endfor %}
                            </div>
                          </div>
                          <div class="megamenu-item megamenu-content-item">
                            <div class="megamenu-content-area">
                              <div class="megamenu-content">
                                <div class="megamenu-links">
                                  <h6 class="megamenu-links-title">{{ b.heading | default: 'Featured' }}</h6>
                                  <ul>
                                    {% if b.text != blank %}
                                      <li>
                                        <a href="{{ b.url | default: '#' }}" class="nav-link-text">{{ b.text }}</a>
                                      </li>
                                    {% endif %}
                                    {% if b.text_2 != blank %}
                                      <li>
                                        <a href="{{ b.url_2 | default: '#' }}" class="nav-link-text">{{ b.text_2 }}</a>
                                      </li>
                                    {% endif %}
                                    <li>
