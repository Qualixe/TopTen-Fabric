{% schema %}
{
  "name": "Header",
  "settings": [
    {
      "type": "image_picker",
      "id": "logo",
      "label": "Logo"
    },
    {
      "type": "text",
      "id": "moible_number",
      "label": "Moible Number",
      "default": "01708156699"
    },
    {
      "type": "link_list",
      "id": "menu",
      "label": "Main Menu",
      "default": "main-menu",
      "info": "Select the menu to display in the header navigation"
    },
    {
      "type": "header",
      "content": "Header Style"
    },
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "Color Scheme",
      "default": "scheme-1"
    },
    {
      "type": "checkbox",
      "id": "enable_sticky_header",
      "label": "Enable Sticky Header",
      "default": true
    }
  ],
  "blocks": [
    {
      "type": "mega_menu",
      "name": "Mega Menu",
      "settings": [
        {
          "type": "text",
          "id": "link",
          "label": "Menu Link Title",
          "info": "Enter the exact menu item title where you want to show this mega menu (case sensitive)"
        },
        {
          "type": "link_list",
          "id": "menu",
          "label": "Mega Menu Content",
          "default": "main-menu",
          "info": "Select the menu that contains your categories (Occasion, Style, Fabric, etc.)"
        },
        {
          "type": "header",
          "content": "Featured Product 1"
        },
        {
          "type": "text",
          "id": "heading",
          "label": "Featured Heading 1",
          "default": "EXPLORE LATEST"
        },
        {
          "type": "text",
          "id": "text",
          "label": "Featured Text 1",
          "default": "Featured Collection"
        },
        {
          "type": "url",
          "id": "url",
          "label": "Featured Link 1"
        },
        {
          "type": "header",
          "content": "Featured Product 2"
        },
        {
          "type": "image_picker",
          "id": "image_2",
          "label": "Featured Image 2"
        },
        {
          "type": "text",
          "id": "heading_2",
          "label": "Featured Heading 2",
          "default": "EXPLORE LATEST"
        },
        {
          "type": "text",
          "id": "text_2",
          "label": "Featured Text 2",
          "default": "Featured Collection"
        },
        {
          "type": "url",
          "id": "url_2",
          "label": "Featured Link 2"
        },
        {
          "type": "header",
          "content": "Mega Menu Settings"
        },
        {
          "type": "range",
          "id": "max_columns",
          "min": 2,
          "max": 4,
          "step": 1,
          "label": "Maximum Columns",
          "default": 3,
          "info": "Maximum number of category columns to display"
        },
        {
          "type": "checkbox",
          "id": "show_featured_products",
          "label": "Show Featured Products",
          "default": true,
          "info": "Display featured product cards on the right side"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Header"
    }
  ]
}
{% endschema %}
