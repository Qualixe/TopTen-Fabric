    <div class="navbar-bottom">
      <div class="container">
        <div class="nav-link-wrapper">
          <div class="nav-link-wrapper-inner">
            <ul class="nav-links">
              {% for link in section.settings.menu.links %}
                {% assign title = link.title | handleize %}
                {% assign has_mega_menu = false %}

                {% comment %} Check if this link has a mega menu {% endcomment %}
                {% for block in section.blocks %}
                  {% assign block_title = block.settings.link | handleize %}
                  {% if title == block_title and block.type == 'mega_menu' %}
                    {% assign has_mega_menu = true %}
                    {% break %}
                  {% endif %}
                {% endfor %}

                <li class="nav-link">
                  {% if has_mega_menu %}
                    <span class="nav-link-text">
                      {{ link.title }}
                    </span>
                  {% else %}
                    <a href="{{ link.url }}" class="nav-link-text">
                      <span>{{ link.title }}</span>
                    </a>
                  {% endif %}

                  {% for block in section.blocks %}
                    {% assign block_title = block.settings.link | handleize %}
                    {% if title == block_title and block.type == 'mega_menu' %}
                      {% render 'megamenu', block: block, top: false %}
                    {% endif %}
                  {% endfor %}
                </li>
              {% endfor %}
            </ul>
          </div>
        </div>
      </div>
    </div>
